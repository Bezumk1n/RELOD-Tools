<Window x:Class="RELOD_Tools.ProgressBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RELOD_Tools"
        mc:Ignorable="d"
        Title="" Height="80" Width="500" ResizeMode="CanMinimize">
    <Grid>
        <ProgressBar x:Name="progressBar" HorizontalAlignment="Center" VerticalAlignment="Center" Width="480" Height="30" ValueChanged="progressBar_ValueChanged"/>
    </Grid>
</Window>
